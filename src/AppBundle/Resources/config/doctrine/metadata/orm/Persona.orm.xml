<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Persona" table="persona">
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="persona_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nacionalidad" type="string" column="nacionalidad" length="1" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cedula" type="string" column="cedula" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="primerApellido" type="string" column="primer_apellido" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="segundoApellido" type="string" column="segundo_apellido" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="primerNombre" type="string" column="primer_nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="segundoNombre" type="string" column="segundo_nombre" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="genero" type="string" column="genero" length="1" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="telefono" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="foto" type="string" column="foto" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
