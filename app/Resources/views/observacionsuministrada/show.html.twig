{% extends 'base.html.twig' %}

{% block body %}
    <h1>Observacionsuministrada</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ observacionSuministrada.id }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ observacionSuministrada.descripcion }}</td>
            </tr>
            <tr>
                <th>Tipo</th>
                <td>{{ observacionSuministrada.tipo }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if observacionSuministrada.fecha %}{{ observacionSuministrada.fecha|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('observacionsuministrada_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('observacionsuministrada_edit', { 'id': observacionSuministrada.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
