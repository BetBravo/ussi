{% extends 'base.html.twig' %}

{% block body %}
    <h1>Profesional</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ profesional.id }}</td>
            </tr>
            <tr>
                <th>Codigossa</th>
                <td>{{ profesional.codigoSsa }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('profesional_index') }}">Back to the list</a>
        </li>
